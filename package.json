{
  "name": "todo",
  "version": "0.0.2",
  "description": "todo app",
  "author": "edeity<edeity@gmail.com>",
  "private": true,
  "homepage": "./",
  "main": "./public/electron.js",
  "dependencies": {
    "animate.css": "^3.7.2",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "fast-json-stringify": "^1.15.3",
    "node-mac-notifier": "^1.2.0",
    "node-notifier": "^5.4.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^11.0.2",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "react-transition-group": "1.x",
    "typescript": "^3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clear": "cross-env rm -rf build && rm -rf dist ",
    "start-app": "cross-env NODE_ENV=development electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build-app": "yarn clear && yarn build && yarn dist",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "favicon.ico"
    },
    "files": [
      {
        "from": "./",
        "to": "./",
        "filter": [
          "**/*",
          "!node_modules"
        ]
      }
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0"
  },
  "__npminstall_done": false
}
